import java.util.Scanner;
class square_root
{
    public static void main(String[] args)
    {
     Scanner sc=new Scanner(System.in);
       
     int a=sc.nextInt();
     for(int i=0;i<a;i++)
     {
         int j=sc.nextInt();
         int sqrt=(int)Math.sqrt(j);
         System.out.println(sqrt);
         
     }
    }


     
}
